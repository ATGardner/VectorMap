{
  "version": 8,
  "name": "IHM",
  "metadata": {"maputnik:renderer": "mbgljs"},
  "sources": {
    "IHM": {
      "type": "vector",
      "url": "https://israelhiking.osm.org.il/data/v3.json"
    },
    "Israel-TerrainRGB": {
      "type": "raster-dem",
      "url": "https://israelhiking.osm.org.il/data/TerrainRGB.json"
    }
  },
  "sprite": "https://raw.githubusercontent.com/IsraelHikingMap/VectorMap/master/Icons/publish/sprite",
  "glyphs": "https://orangemug.github.io/font-glyphs/glyphs/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "layout": {"visibility": "visible"},
      "paint": {"background-color": "rgba(250, 247, 225, 1)"}
    },
    {
      "id": "area-residential",
      "type": "fill",
      "source": "IHM",
      "source-layer": "landuse",
      "filter": ["all", ["in", "class", "residential", "industrial"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-outline-color": "rgba(102, 102, 102, 0.3)",
        "fill-color": "rgba(242, 242, 242, 1)"
      }
    },
    {
      "id": "area-sand",
      "type": "fill",
      "source": "IHM",
      "source-layer": "landcover",
      "filter": ["all", ["==", "subclass", "sand"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "rgba(255, 236, 139, 0.25)",
        "fill-outline-color": "rgba(0, 0, 0, 0)"
      }
    },
    {
      "id": "area-wood",
      "type": "fill",
      "source": "IHM",
      "source-layer": "landcover",
      "filter": ["all", ["==", "class", "wood"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "rgba(145, 230, 145, 0.5)",
        "fill-outline-color": "rgba(0, 0, 0, 0)"
      }
    },
    {
      "id": "area-farmland",
      "type": "fill",
      "source": "IHM",
      "source-layer": "landcover",
      "filter": ["all", ["==", "class", "farmland"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "#E7E9C9",
        "fill-outline-color": "rgba(0, 0, 0, 0)"
      }
    },
    {
      "id": "area-plantation",
      "type": "fill",
      "source": "IHM",
      "source-layer": "landcover",
      "filter": ["all", ["==", "subclass", "orchard"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "rgba(19, 196, 42, 0.4)",
        "fill-outline-color": "rgba(0, 0, 0, 0)"
      }
    },
    {
      "id": "area-beach",
      "type": "fill",
      "source": "IHM",
      "source-layer": "landcover",
      "filter": ["all", ["==", "subclass", "beach"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "#FAFAA0",
        "fill-outline-color": "rgba(0, 0, 0, 0)"
      }
    },
    {
      "id": "area-grass",
      "type": "fill",
      "source": "IHM",
      "source-layer": "landcover",
      "filter": ["all", ["==", "class", "grass"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "rgba(233, 253, 204, 1)",
        "fill-outline-color": "rgba(0, 0, 0, 0)"
      }
    },
    {
      "id": "area-cemetery",
      "type": "fill",
      "source": "IHM",
      "source-layer": "landuse",
      "minzoom": 0,
      "maxzoom": 24,
      "filter": ["all", ["==", "class", "cemetery"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "#CCCCCC",
        "fill-outline-color": "rgba(0, 0, 0, 0)"
      }
    },
    {
      "id": "area-quarry",
      "type": "fill",
      "source": "IHM",
      "source-layer": "landcover",
      "filter": ["all", ["==", "subclass", "quarry"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "#BFBFBF",
        "fill-outline-color": "rgba(0, 0, 0, 0)"
      }
    },
    {
      "id": "area-military-training",
      "type": "fill",
      "source": "IHM",
      "source-layer": "landuse",
      "minzoom": 12,
      "filter": [
        "all",
        ["==", "class", "military"],
        ["==", "military", "training_area"]
      ],
      "layout": {"visibility": "none"},
      "paint": {
        "fill-outline-color": "red",
        "fill-pattern": "red_nwse_pattern",
        "fill-opacity": 0.2
      }
    },
    {
      "id": "area-military",
      "type": "fill",
      "source": "IHM",
      "source-layer": "landuse",
      "filter": [
        "all",
        ["==", "class", "military"],
        ["!=", "military", "training_area"]
      ],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-outline-color": "red",
        "fill-pattern": "red_nesw_pattern",
        "fill-opacity": 0.2
      }
    },
    {
      "id": "Terrain",
      "type": "hillshade",
      "source": "Israel-TerrainRGB",
      "layout": {"visibility": "visible"}
    },
    {
      "id": "casing-steps",
      "type": "line",
      "source": "IHM",
      "source-layer": "transportation",
      "filter": ["all", ["==", "subclass", "steps"]],
      "layout": {
        "visibility": "visible",
        "line-cap": "butt",
        "line-join": "miter"
      },
      "paint": {
        "line-color": "white",
        "line-width": {"stops": [[13, 2], [14, 5.5]]}
      }
    },
    {
      "id": "casing-path",
      "type": "line",
      "source": "IHM",
      "source-layer": "transportation",
      "minzoom": 13,
      "filter": ["all", ["==", "class", "path"]],
      "layout": {
        "visibility": "visible",
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "white",
        "line-width": {"stops": [[13, 2], [16, 2.5]]}
      }
    },
    {
      "id": "casing-track",
      "type": "line",
      "source": "IHM",
      "source-layer": "transportation",
      "minzoom": 13,
      "filter": ["all", ["==", "class", "track"]],
      "layout": {
        "visibility": "visible",
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-width": {"stops": [[14, 1], [16, 1.8]]},
        "line-color": "rgba(247, 244, 242, 1)"
      }
    },
    {
      "id": "casing-minor",
      "type": "line",
      "source": "IHM",
      "source-layer": "transportation",
      "minzoom": 13,
      "filter": ["all", ["in", "class", "minor", "service"]],
      "layout": {
        "visibility": "visible",
        "line-join": "round",
        "line-cap": "round"
      },
      "paint": {
        "line-color": ["coalesce", ["get", "colour"], "#808080"],
        "line-width": {"stops": [[13, 2], [14, 3.5], [16, 6]]}
      }
    },
    {
      "id": "casing-tertiary",
      "type": "line",
      "source": "IHM",
      "source-layer": "transportation",
      "minzoom": 12,
      "filter": ["all", ["==", "class", "tertiary"]],
      "layout": {
        "visibility": "visible",
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": ["to-color", ["get", "colour"], "#808080"],
        "line-width": {"stops": [[13, 3.5], [16, 6]]}
      }
    },
    {
      "id": "casing-seconary",
      "type": "line",
      "source": "IHM",
      "source-layer": "transportation",
      "minzoom": 12,
      "filter": ["all", ["==", "class", "secondary"]],
      "layout": {
        "visibility": "visible",
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": ["to-color", ["get", "colour"], "#808080"],
        "line-width": {"stops": [[12, 2], [13, 3.5], [16, 6]]}
      }
    },
    {
      "id": "casing-primary",
      "type": "line",
      "source": "IHM",
      "source-layer": "transportation",
      "minzoom": 10,
      "filter": ["all", ["==", "class", "primary"]],
      "layout": {
        "visibility": "visible",
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": ["to-color", ["get", "colour"], "#808080"],
        "line-width": {"stops": [[10, 1], [12, 1.5], [16, 6]]}
      }
    },
    {
      "id": "casing-trunk",
      "type": "line",
      "source": "IHM",
      "source-layer": "transportation",
      "minzoom": 7,
      "filter": ["all", ["==", "class", "trunk"]],
      "layout": {
        "visibility": "visible",
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": ["to-color", ["get", "colour"], "#808080"],
        "line-width": {"stops": [[7, 1], [9, 1.5], [16, 6]]}
      }
    },
    {
      "id": "casing-motorway",
      "type": "line",
      "source": "IHM",
      "source-layer": "transportation",
      "minzoom": 6,
      "filter": ["all", ["==", "class", "motorway"]],
      "layout": {
        "visibility": "visible",
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": ["to-color", ["get", "colour"], "#808080"],
        "line-width": {"stops": [[7, 1], [9, 1.5], [16, 6]]}
      }
    },
    {
      "id": "water-line-intermittent",
      "type": "line",
      "source": "IHM",
      "source-layer": "waterway",
      "filter": ["all", ["==", "intermittent", 1]],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "#8EBDF0",
        "line-width": {"stops": [[9, 2], [10, 3]]},
        "line-dasharray": [4, 1]
      }
    },
    {
      "id": "water-line",
      "type": "line",
      "source": "IHM",
      "source-layer": "waterway",
      "filter": ["all", ["!=", "intermittent", 1]],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "#8EBDF0",
        "line-width": {"stops": [[9, 2], [10, 3]]}
      }
    },
    {
      "id": "water-area-intermittent",
      "type": "fill",
      "source": "IHM",
      "source-layer": "water",
      "filter": ["all", ["==", "intermittent", 1]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-pattern": "intermittent_pattern",
        "fill-outline-color": "#1E80E2"
      }
    },
    {
      "id": "water-area",
      "type": "fill",
      "source": "IHM",
      "source-layer": "water",
      "filter": ["all", ["!=", "intermittent", 1]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "#8EBDF0",
        "fill-antialias": true,
        "fill-outline-color": "#1E80E2"
      }
    },
    {
      "id": "boundary-country",
      "type": "line",
      "source": "IHM",
      "source-layer": "boundary",
      "filter": ["all", ["==", "admin_level", 2], ["!=", "maritime", 1]],
      "layout": {"visibility": "visible"}
    },
    {
      "id": "aeroway-helipad",
      "type": "fill",
      "source": "IHM",
      "source-layer": "aeroway",
      "minzoom": 15,
      "maxzoom": 24,
      "filter": ["all", ["==", "class", "helipad"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-opacity": 1,
        "fill-color": "rgba(96, 96, 96, 1)",
        "fill-outline-color": "rgba(64, 64, 64, 1)"
      }
    },
    {
      "id": "aeroway-taxiway",
      "type": "line",
      "source": "IHM",
      "source-layer": "aeroway",
      "minzoom": 13,
      "maxzoom": 24,
      "filter": ["all", ["==", "class", "taxiway"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "rgba(96, 96, 96, 1)",
        "line-width": {"stops": [[13, 3], [14, 6], [15, 12]]}
      }
    },
    {
      "id": "aeroway-runway",
      "type": "line",
      "source": "IHM",
      "source-layer": "aeroway",
      "filter": ["all", ["==", "class", "runway"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "rgba(96, 96, 96, 1)",
        "line-width": {"stops": [[11, 3], [12, 4], [13, 8], [14, 16], [15, 32]]}
      }
    },
    {
      "id": "building",
      "type": "fill",
      "source": "IHM",
      "source-layer": "building",
      "minzoom": 15,
      "maxzoom": 24,
      "filter": ["all"],
      "layout": {"visibility": "visible"},
      "paint": {"fill-opacity": 1, "fill-color": "rgba(191, 191, 191, 1)"}
    },
    {
      "id": "barrier-wall",
      "type": "line",
      "source": "IHM",
      "source-layer": "linear_barrier",
      "minzoom": 14,
      "maxzoom": 24,
      "filter": ["all", ["in", "class", "wall", "city_wall"]],
      "layout": {"visibility": "visible"},
      "paint": {"line-color": "black", "line-width": 1.5}
    },
    {
      "id": "barrier-fence-line",
      "type": "line",
      "source": "IHM",
      "source-layer": "linear_barrier",
      "minzoom": 14,
      "maxzoom": 24,
      "filter": ["all", ["==", "class", "fence"]],
      "layout": {"visibility": "visible"},
      "paint": {"line-color": "rgba(26, 26, 26, 1)"}
    },
    {
      "id": "barrier-fence-x",
      "type": "symbol",
      "source": "IHM",
      "source-layer": "linear_barrier",
      "minzoom": 14,
      "maxzoom": 24,
      "filter": ["all", ["==", "class", "fence"]],
      "layout": {
        "visibility": "visible",
        "symbol-placement": "line",
        "icon-image": "cross_pattern",
        "symbol-spacing": 1,
        "icon-allow-overlap": true,
        "icon-size": 0.2
      },
      "paint": {}
    },
    {
      "id": "trail-israel",
      "type": "line",
      "source": "IHM",
      "source-layer": "trail",
      "filter": [
        "all",
        ["==", "osmc_symbol", "blue:white:blue_stripe:orange_stripe_left"]
      ],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "rgba(255, 210, 136, 1)",
        "line-width": {"stops": [[12, 3], [13, 7], [16, 12]]}
      }
    },
    {
      "id": "trail-kinneret-bike-line",
      "type": "line",
      "source": "IHM",
      "source-layer": "trail",
      "minzoom": 8,
      "filter": ["all", ["==", "osmc_symbol", "yellow:yellow_circle:blue_dot"]],
      "layout": {"visibility": "none"},
      "paint": {
        "line-color": "blue",
        "line-width": {"stops": [[7, 1], [10, 1.5], [11, 2], [15, 3]]},
        "line-offset": {"stops": [[11, 0], [12, -1], [14, -4]]}
      }
    },
    {
      "id": "trail-kinneret-bike-dash",
      "type": "line",
      "source": "IHM",
      "source-layer": "trail",
      "minzoom": 8,
      "filter": ["all", ["==", "osmc_symbol", "yellow:yellow_circle:blue_dot"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-dasharray": [4, 4],
        "line-color": "yellow",
        "line-width": {"stops": [[7, 1], [10, 1.5], [11, 2], [15, 3]]},
        "line-offset": {"stops": [[11, 0], [12, -1], [14, -4]]}
      }
    },
    {
      "id": "trail-regional-line",
      "type": "line",
      "source": "IHM",
      "source-layer": "trail",
      "minzoom": 8,
      "filter": [
        "all",
        ["==", "network", "rwn"],
        ["!=", "osmc_symbol", "yellow:yellow_circle:blue_dot"]
      ],
      "layout": {
        "visibility": "visible",
        "line-join": "miter",
        "line-cap": "butt"
      },
      "paint": {
        "line-color": [
          "match",
          ["get", "osmc_symbol"],
          "purple:white:purple_stripe",
          "purple",
          "orange:white:orange_stripe",
          "orange",
          "yellow:white:yellow_stripe",
          "yellow",
          "yellow:yellow_circle:blue_dot",
          "blue",
          "purple:white:purple_stripe:blue_stripe_left",
          "#1765AD",
          "blue:white:blue_stripe:green_stripe_left",
          "#4ca64c",
          [
            "match",
            ["get", "name"],
            "שביל ואדיות חיפה",
            "red",
            "שביל ירושלים",
            "yellow",
            "rgba(0,0,0,0)"
          ]
        ],
        "line-width": {"stops": [[7, 1], [10, 1.5], [11, 2], [15, 3]]},
        "line-offset": {"stops": [[11, 0], [12, 1], [14, 4]]}
      }
    },
    {
      "id": "trail-regional-dash",
      "type": "line",
      "source": "IHM",
      "source-layer": "trail",
      "minzoom": 8,
      "filter": [
        "all",
        ["==", "network", "rwn"],
        ["!=", "osmc_symbol", "yellow:yellow_circle:blue_dot"]
      ],
      "layout": {
        "visibility": "visible",
        "line-join": "miter",
        "line-cap": "butt"
      },
      "paint": {
        "line-color": [
          "match",
          ["get", "osmc_symbol"],
          "yellow:yellow_circle:blue_dot",
          "yellow",
          "purple:white:purple_stripe:blue_stripe_left",
          "#7B5CA0",
          "blue:white:blue_stripe:green_stripe_left",
          "blue",
          [
            "match",
            ["get", "name"],
            "שביל ואדיות חיפה",
            "green",
            "שביל ירושלים",
            "blue",
            "rgba(0,0,0,0)"
          ]
        ],
        "line-dasharray": [4, 4],
        "line-width": {"stops": [[7, 1], [10, 1.5], [11, 2], [15, 3]]},
        "line-offset": {"stops": [[11, 0], [12, 1], [14, 4]]}
      }
    },
    {
      "id": "trail-line",
      "type": "line",
      "source": "IHM",
      "source-layer": "trail",
      "minzoom": 11,
      "maxzoom": 13,
      "filter": ["all", ["in", "network", "lwn", "rwn"]],
      "layout": {
        "visibility": "visible",
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-width": 1,
        "line-color": [
          "match",
          ["get", "osmc_symbol"],
          "black:white:black_stripe",
          "black",
          "red:white:red_stripe",
          "red",
          "green:white:green_stripe",
          "green",
          "blue:white:blue_stripe",
          "blue",
          "purple:white:purple_stripe",
          "purple",
          "orange:white:orange_stripe",
          "orange",
          "yellow:white:yellow_stripe",
          "yellow",
          "rgba(0,0,0,0)"
        ]
      }
    },
    {
      "id": "road-remaining",
      "type": "line",
      "source": "IHM",
      "source-layer": "transportation",
      "filter": [
        "none",
        ["==", "class", "motorway"],
        ["==", "class", "trunk"],
        ["==", "class", "primary"],
        ["==", "class", "secondary"],
        ["==", "class", "tertiary"]
      ],
      "layout": {
        "visibility": "none",
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "rgba(255, 196, 0, 1)",
        "line-width": {"stops": [[7, 0.5], [16, 1]]}
      }
    },
    {
      "id": "path-steps",
      "type": "line",
      "source": "IHM",
      "source-layer": "transportation",
      "filter": ["all", ["==", "subclass", "steps"]],
      "layout": {
        "visibility": "visible",
        "line-cap": "butt",
        "line-join": "miter"
      },
      "paint": {
        "line-dasharray": [0.2, 0.3],
        "line-width": {"stops": [[13, 1.5], [14, 5]]},
        "line-color": ["to-color", ["get", "colour"], "#8e5632"]
      }
    },
    {
      "id": "path-footway",
      "type": "line",
      "source": "IHM",
      "source-layer": "transportation",
      "filter": ["all", ["==", "subclass", "footway"]],
      "layout": {
        "visibility": "visible",
        "line-cap": "butt",
        "line-join": "miter"
      },
      "paint": {
        "line-dasharray": [1, 1],
        "line-width": {"stops": [[13, 1.5], [14, 2]]},
        "line-color": ["to-color", ["get", "colour"], "#8e5632"]
      }
    },
    {
      "id": "path-cycleway",
      "type": "line",
      "source": "IHM",
      "source-layer": "transportation",
      "filter": ["all", ["==", "subclass", "cycleway"]],
      "layout": {
        "visibility": "visible",
        "line-cap": "butt",
        "line-join": "miter"
      },
      "paint": {
        "line-dasharray": [3, 1, 1, 1],
        "line-width": {"stops": [[13, 1.5], [14, 2]]},
        "line-color": ["to-color", ["get", "colour"], "#aa8166"]
      }
    },
    {
      "id": "path-path",
      "type": "line",
      "source": "IHM",
      "source-layer": "transportation",
      "filter": ["all", ["in", "subclass", "path", "pedestrian", "platform"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-dasharray": [3, 1],
        "line-width": {"stops": [[13, 1.5], [14, 2]]},
        "line-color": ["to-color", ["get", "colour"], "#aa8166"]
      }
    },
    {
      "id": "track-right",
      "type": "line",
      "source": "IHM",
      "source-layer": "transportation",
      "minzoom": 13,
      "filter": ["all", ["==", "class", "track"]],
